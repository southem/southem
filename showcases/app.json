{
  "expo": {
    "name": "UI Southem Showcases",
    "slug": "ui-southem-showcases",
    "description": "UI Southem Documentation Showcases",
    "sdkVersion": "48.0.0",
    "privacy": "public",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "githubUrl": "https://github.com/southem/southem",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "src/assets/images/icon-app.png",
    "splash": {
      "image": "src/assets/images/image-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 15000,
      "url": "https://u.expo.dev/14f86041-c0c6-4796-8ab9-dcddf6991214"
    },
    "assetBundlePatterns": [
      "src/assets/images/*",
      "src/assets/fonts",
      "src/layouts/**/assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "ui.southem.showcases",
      "appStoreUrl": "https://itunes.apple.com/us/app/ui-southem-showcases/id1246173236",
      "buildNumber": "3"
    },
    "android": {
      "versionCode": 3,
      "package": "ui.southem.showcases",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=ui.southem.showcases",
      "adaptiveIcon": {
        "foregroundImage": "src/assets/images/icon-app.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS",
        "WAKE_LOCK",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#F7F9FC"
    },
    "web": {
      "favicon": "src/assets/images/icon-app.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "src/assets/images/icon-app.png",
          "color": "#ffffff",
          "sounds": [
            "src/assets/notifications/notification.wav"
          ]
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "69c2b9ad-28d9-47ec-a612-e26a3bc96220"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    }
  }
}
