    "@types/react-native": "^0.70.8",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "3.0.2",
    "babel-jest": "^29.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "change-case": "^4.1.2",
