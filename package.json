    "@types/react-native": "ts3.7",
    "@types/react-test-renderer": "ts3.7",
    "@types/rimraf": "ts3.7",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "change-case": "^3.1.0",
