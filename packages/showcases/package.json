{
  "name": "@southem/showcases",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://github.com/southem/southem/tree/master/packages/showcases#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/southem/southem.git",
    "directory": "packages/showcases"
  },
  "bugs": {
    "url": "https://github.com/southem/southem/issues"
  },
  "workspaces": {
    "nohoist": [
      "*",
      "*/**"
    ]
  },
  "scripts": {
    "start": "yarn run env -- dev && expo start --tunnel",
    "start-dev": "yarn run env -- dev && expo start --dev-client",
    "android": "yarn run env -- dev && expo run:android",
    "ios": "yarn run env -- dev && expo run:ios",
    "prod": "yarn env -- prod && expo start -c",
    "web": "yarn run env -- dev && expo start --web",
    "ios-simulator": "eas build --profile development-simulator --platform ios",
    "eject": "expo eject",
    "build": "expo build",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "build:web": "expo build:web",
    "clean": "rimraf ./web-build",
    "env": "node ./env/set-env.js"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.2",
    "@react-navigation/drawer": "^6.5.6",
    "@react-navigation/material-top-tabs": "^6.5.1",
    "@react-navigation/native": "^6.1.1",
    "@react-navigation/stack": "^6.3.10",
    "@react-navigation/native-stack": "^6.9.8",
    "awesome-debounce-promise": "^2.1.0",
    "expo": "^47.0.12",
    "expo-analytics": "^1.0.18",
    "expo-device": "^5.0.0",
    "expo-linear-gradient": "^12.0.1",
    "expo-network": "^5.0.0",
    "expo-notifications": "^0.17.0",
    "expo-status-bar": "^1.4.2",
    "expo-updates": "^0.15.6",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.2.tar.gz",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.1",
    "react-native-reanimated": "^2.13.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-tab-view": "^3.3.4",
    "react-native-web": "^0.18.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/plugin-proposal-decorators": "^7.20.7",
    "@expo/webpack-config": "^0.17.4",
    "@types/color": "^3.0.3",
    "@types/react": "^18.0.26",
    "@types/react-native": "^0.70.8",
    "babel-preset-expo": "^9.2.2",
    "typescript": "^4.9.4"
  }
}
